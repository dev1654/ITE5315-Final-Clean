<h2>{{title}}</h2>
<p>Showing {{data.length}} properties</p>

{{#if data.length}}
    <table border="1">
        <tr>
            <th>Image</th>
            <th>Name</th>
            <th>Neighbourhood</th>
            <th>Price</th>
        </tr>
        {{#each data}}
            <tr>
                <td><img src="{{this.thumbnail}}" width="100"></td>
                <td>{{this.NAME}}</td>
                <td>{{this.neighbourhood}}</td>
                <td>{{this.price}}</td>
            </tr>
        {{/each}}
    </table>
{{else}}
    <p>No data available</p>
{{/if}}